{
    "proj_name" : "DevAssistant",
    "dir_proj" : "../../app/",
    "options": {
        "WITH_QT" : "ON",
        "WITH_PY" : "ON"
    },
    "dir_codes":[
        {
            "group" : "QtAPP",
            "group_dir" : "/src/app",
            "modules": [
                {
                    "module" : "DevAssistant",
                    "option" : "ON",
                    "gen_lib" : "NO",
                    "deps" : [
                        "QtProject",
                        "QtExample",
                        "QtPractical",
                        "QtCustom",
                        "QtPlan",
                        "QtHelp",
                        "QtEnvKit",
                        "QtCoreKit",
                        "DACoreKit"
                    ]
                }
            ]
        },
        {
            "group" : "QtKit",
            "group_dir" : "/src/qtmodules",
            "modules": [
                {
                    "module" : "QtProject",
                    "option" : "ON",
                    "gen_lib" : "YES",
                    "deps" : [
                        "QtEnvKit",
                        "QtCoreKit"
                    ]
                },
                {
                    "module" : "QtExample",
                    "option" : "ON",
                    "gen_lib" : "YES"
                },
                {
                    "module" : "QtPractical",
                    "option" : "ON",
                    "gen_lib" : "YES"
                },
                {
                    "module" : "QtCustom",
                    "option" : "ON",
                    "gen_lib" : "YES"
                },
                {
                    "module" : "QtPlan",
                    "option" : "ON",
                    "gen_lib" : "YES"
                },
                {
                    "module" : "QtHelp",
                    "option" : "ON",
                    "gen_lib" : "YES"
                },
                {
                    "module" : "QtEnvKit",
                    "option" : "ON",
                    "gen_lib" : "YES",
                    "deps" : [
                        "QtCoreKit"
                    ]
                },
                {
                    "module" : "QtCoreKit",
                    "option" : "ON",
                    "gen_lib" : "YES"
                }
            ]
        },
        {
            "group" : "Kit",
            "group_dir" : "/src/kits",
            "modules": [
                {
                    "module" : "DACoreKit",
                    "option" : "ON",
                    "gen_lib" : "YES"
                }
            ]
        },
        {
            "group" : "Lib",
            "group_dir" : "/deps",
            "modules": [
                {
                    "module" : "QtCryptoKit",
                    "option" : "ON"
                }
            ]
        }
    ],
    "dir_help": {
        "conf" : "conf", 
        "scripts" : "scripts", 
        "tools" : "tools", 
        "doc" : "doc"
    }
}